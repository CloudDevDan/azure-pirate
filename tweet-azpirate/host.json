{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "managedDependency": {
    "enabled": true
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "serviceBus": {
    "autoCompleteMessages": true,
    "maxAutoLockRenewalDuration": "00:05:00",
    "maxConcurrentCalls": 1,
    "prefetchCount": 1
  }
}